{
  "name": "nome-do-meu-app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.7",
    "@mui/styled-engine-sc": "^5.1.0",
    "@mui/x-data-grid": "^5.2.2",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.24.0",
    "concurrently": "^6.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "i18next": "^21.6.4",
    "moment": "^2.29.1",
    "pm2": "^4.5.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.23.0",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "6",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn run dev:server\" \"yarn run dev:client\"",
    "build": "yarn run build:server && yarn run build:client",
    "build:server": "cd ./server && yarn && yarn build",
    "dev:client": "react-scripts start",
    "dev:server": "yarn run build:server && cd ./server && yarn start",
    "build:client": "yarn && react-scripts build",
    "start": "pm2-runtime start pm2.json --env production",
    "serve:kill": "pm2 kill"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint-plugin-react-hooks": "^4.3.0"
  }
}
